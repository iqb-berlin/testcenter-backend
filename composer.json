{
  "name": "iqb/testcenter-api",
  "description": "The backend for the IQB-Testcenter application(s).",
  "version": "10.0.1",
  "extra": {
    "broadcastingServiceVersionExpected": "2.0.2"
  },
  "homepage": "https://iqb-testcenter.de",
  "type": "project",
  "support": {
    "issues": "https://github.com/iqb-berlin/testcenter-backend/issues",
    "docs": "https://iqb-berlin.github.io/testcenter-backend/"
  },
  "repositories": [
    {
      "type": "github",
      "url": "https://github.com/iqb-berlin/testcenter-backend.git"
    },
    {
      "type": "package",
      "package": {
        "name": "iqb-berlin/verona-player-simple",
        "version": "10.0.1",
        "source": {
          "url": "https://github.com/iqb-berlin/verona-player-simple.git",
          "type": "git",
          "reference": "1.0.1"
        }
      }
    }
  ],
  "readme": "README.md",
  "license": "MIT",
  "require": {
    "slim/slim": "^3.12.0",
    "php": "^7.4",
    "ext-pdo_mysql": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-zip": "*",
    "ext-libxml": "*",
    "ext-simplexml": "*",
    "ext-xmlreader": "*",
    "ext-curl": "*",
    "ext-dom": "*"
  },
  "suggest": {
    "ext-apache": "*"
  },
  "require-dev": {
    "mikey179/vfsstream": "^1.6",
    "phpunit/phpunit":  "^8",
    "iqb-berlin/verona-player-simple": "1.0.1",
    "ext-propro":  "*",
    "ext-raphf":  "*",
    "ext-http":  "*"
  }
}
